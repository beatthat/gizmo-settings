{
  "name": "gizmo-settings",
  "version": "1.0.0",
  "description": "GizmoSettings is a util component thats it easier to stencil gizmos for complex UI setups",
  "config": {
    "scope": "ape"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node scripts/postinstall.js",
    "preinstall:test": "rm -f *.tgz",
    "install:test": "npm pack && cp ${npm_package_name}-${npm_package_version}.tgz ${npm_package_name}-latest.tgz && cd test && npm run clean && npm install",
    "clean": "rm -f *.tgz package-lock.json && cd test && npm run clean",
    "template:update": "node scripts/template-update.js",
    "cp:test2src": "cd test && node scripts/cp-test2src.js",
    "overwrite:test2src": "cd test && node scripts/overwrite-test2src.js",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beatthat/gizmo-settings.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beatthat/gizmo-settings/issues"
  },
  "homepage": "https://github.com/beatthat/gizmo-settings#readme",
  "dependencies": {
    "color-ext": "github:beatthat/color-ext#semver:^1.0.2",
    "path": "^0.12.7",
    "pools": "github:beatthat/pools#semver:^1.0.0",
    "rect-utils": "github:beatthat/rect-utils#semver:^1.0.0",
    "unity-npm-utils": "github:beatthat/unity-npm-utils#semver:^1.0.0"
  },
  "files": [
    "index.js",
    "scripts",
    "src"
  ],
  "devDependencies": {}
}
